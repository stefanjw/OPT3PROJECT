<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ShopController">
   <children>
      <TableView fx:id="ArtikelenTableview" layoutX="139.0" layoutY="60.0" prefHeight="256.0" prefWidth="323.0">
        <columns>
          <TableColumn fx:id="naamKolom" text="naam" />
          <TableColumn fx:id="prijsKolom" prefWidth="60.0" text="prijs" />
            <TableColumn fx:id="artikelnummerKolom" prefWidth="109.0" text="artikelnummer" />
          <TableColumn fx:id="aantalKolom" prefWidth="72.0" text="aantal" />
        </columns>
      </TableView>
      <Button fx:id="BackButton" layoutX="45.0" layoutY="334.0" mnemonicParsing="false" onAction="#GoingBackToMenu" text="Back" />
      <Button fx:id="addProduct" layoutX="249.0" layoutY="334.0" mnemonicParsing="false" onAction="#addProductToWinkelmand" text="AddProduct" />
      <Button fx:id="GoToBonButton" layoutX="481.0" layoutY="334.0" mnemonicParsing="false" onAction="#GoToBon" prefHeight="25.0" prefWidth="68.0" text="CheckOut" />
      <Button fx:id="RemoveButton" layoutX="157.0" layoutY="334.0" mnemonicParsing="false" onAction="#removeProduct" text="Remove" />
      <Text fx:id="productCountText" layoutX="462.0" layoutY="71.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="131.0" />
   </children>
</AnchorPane>
